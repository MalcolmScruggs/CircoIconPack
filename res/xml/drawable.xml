    <category title="Colors" />
    <item drawable="ic_calendar_blue" />
    <item drawable="ic_calendar_green" />
    <item drawable="ic_calendar_red" />
    <item drawable="ic_calendar_alt_blue" />
    <item drawable="ic_phone_blue" />
    <item drawable="ic_phone_green" />
