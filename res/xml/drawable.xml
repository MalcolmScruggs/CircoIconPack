    order of app name -->

    <category title="System" />
    <item drawable="ic_calculator" />
    <item drawable="ic_calendar" />
    <item drawable="ic_calendar_alt" />
    <item drawable="ic_camera" />
    <item drawable="ic_clock" />
    <item drawable="ic_downloads" />
    <item drawable="ic_phone" />
